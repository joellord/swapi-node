  },
  "keywords": [
    "swapi",
    "star wars"
  ],
  "license": "BSD",
  "devDependencies": {
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "nock": "^3.3.2",
    "nsp": "^2.0.2",
    "should": "^7.1.1"
  },
  "dependencies": {
    "request": "^2.67.0"
  }
